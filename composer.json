{
    "name": "setono/tag-bag-twig",
    "type": "library",
    "description": "Twig extension for the tag bag library including a tag and a renderer",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": "^7.2",
        "setono/tag-bag": "^1.3",
        "twig/twig": "^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.4",
        "setono/code-quality-pack": "^1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Setono\\TagBag\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Setono\\TagBag\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse -c phpstan.neon -l max src",
        "check-style": "vendor/bin/ecs check --ansi src/ tests/",
        "fix-style": "vendor/bin/ecs check --fix --ansi src/ tests/",
        "phpunit": "vendor/bin/phpunit"
    }
}
